#!/bin/bash

docker run --name roach-warden  -d --link spawning-pool:db -p 8081:80 phpmyadmin/phpmyadmin

# Launches the 'phpmyadmin' container and link it with the mysql container 'spawning-pool' and name it 'roach-warden'
